{
  "name": "@edusphere/db",
  "version": "0.1.0",
  "private": true,
  "description": "Database layer - Drizzle ORM + PostgreSQL + Apache AGE + pgvector",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./schema": {
      "require": "./dist/schema/index.js",
      "types": "./dist/schema/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "generate": "drizzle-kit generate",
    "migrate": "tsx src/migrate.ts",
    "push": "drizzle-kit push",
    "studio": "drizzle-kit studio",
    "seed": "tsx src/seed.ts",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "drizzle-orm": "^0.39.3",
    "pg": "^8.14.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@edusphere/tsconfig": "workspace:*",
    "@types/node": "^22.10.7",
    "@types/pg": "^8.11.11",
    "@vitest/coverage-v8": "^3.2.4",
    "drizzle-kit": "^0.30.2",
    "tsx": "^4.19.2",
    "typescript": "^5.8.2",
    "vitest": "^3.2.4"
  }
}
